<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.fragment.fication.activity.Merchantentry">
    <include
        android:id="@+id/inv_f"
        layout="@layout/common_top_view" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/huise"></View>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/re"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_14"
                android:layout_marginTop="@dimen/dp_14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="商户信息"
                android:textSize="@dimen/dp_14"
                android:textColor="@color/v_bank"/>
        </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative1"
        android:layout_below="@+id/re"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="公司名称:"
            android:textSize="@dimen/dp_14"/>
        <EditText
            android:id="@+id/tv_company_name"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:hint="请输入公司名称"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:layout_marginLeft="@dimen/dp_99"
            android:textSize="@dimen/dp_14"/>
    </RelativeLayout>
    <View
        android:id="@+id/view1"
        android:layout_below="@id/relative1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/bt_regist_back">
    </View>

    <RelativeLayout
        android:id="@+id/relative2"
        android:layout_below="@id/view1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="员工人数："
            android:textSize="@dimen/dp_14"/>
        <EditText
            android:id="@+id/tv_staff_Number"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:hint="输入员工人数"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:layout_marginLeft="@dimen/dp_99"
            android:textSize="@dimen/dp_14"/>
    </RelativeLayout>
    <View
        android:id="@+id/view2"
        android:layout_below="@id/relative2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/bt_regist_back"></View>
    <RelativeLayout
        android:id="@+id/relative3"
        android:layout_below="@id/view2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45">
    <TextView
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginBottom="@dimen/dp_14"
        android:layout_marginTop="@dimen/dp_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="纳税人识别号："
        android:textSize="@dimen/dp_14"/>
        <EditText
            android:id="@+id/tv_shibie_Number"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:hint="234567876543234567"
            android:inputType="number"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:layout_marginLeft="@dimen/dp_119"
            android:textSize="@dimen/dp_14"
            />
    </RelativeLayout>
    <View
        android:id="@+id/view3"
        android:layout_below="@id/relative3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/bt_regist_back"></View>
    <RelativeLayout
        android:id="@+id/relative4"
        android:layout_below="@id/view3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/dp_14"
            android:text="经营范围："/>
        <EditText
            android:id="@+id/tv_management"
            android:textSize="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_marginLeft="@dimen/dp_83"
            android:layout_width="@dimen/dp_267"
            android:layout_height="@dimen/dp_184"
            android:inputType="textMultiLine"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:hint="经营范围是指国家允许企业生产和经营的商品类别、品种及服务项目，反映企业业务活动的内容和生产经营方向，是企业业务活动范围的法律界"/>
    </RelativeLayout>
    <View
        android:id="@+id/view4"
        android:layout_below="@id/relative4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/bt_regist_back"></View>
    <RelativeLayout
        android:id="@+id/relative5"
        android:layout_below="@id/view4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="法人身份证姓名："
            android:textSize="@dimen/dp_14"/>
        <EditText
            android:id="@+id/tv_name"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:hint="请输入姓名"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:layout_marginLeft="@dimen/dp_131"
            android:textSize="@dimen/dp_14"/>
    </RelativeLayout>
    <View
        android:id="@+id/view5"
        android:layout_below="@id/relative5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/bt_regist_back"></View>
    <RelativeLayout
        android:id="@+id/relative6"
        android:layout_below="@id/view5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_45">
    <TextView
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginBottom="@dimen/dp_14"
        android:layout_marginTop="@dimen/dp_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="法人身份证号："
        android:textSize="@dimen/dp_14"/>
        <EditText
            android:id="@+id/tv_id_number"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:hint="1234567890876543"
            style="?android:attr/textViewStyle"
            android:background="@null"
            android:inputType="number"
            android:layout_marginLeft="@dimen/dp_118"
            android:textSize="@dimen/dp_14"/>
    </RelativeLayout>
    <View
        android:id="@+id/view6"
        android:layout_below="@id/relative6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_8"
        android:background="@color/bt_regist_back"></View>

    <RelativeLayout
        android:id="@+id/relative7"
        android:layout_below="@id/view6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传身份证人像面"
            android:textSize="@dimen/dp_14"
            android:textColor="@color/v_bank"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/relative8"
        android:layout_below="@id/relative7"
        android:layout_width="match_parent"
        android:layout_height="908dp">
    <ImageView
        android:id="@+id/im_shengfenzhengoen"
        android:layout_width="@dimen/dp_202"
        android:layout_height="@dimen/dp_128"
        android:layout_marginLeft="@dimen/dp_85"
        android:layout_marginRight="@dimen/dp_89"
        android:src="@mipmap/shengfenzhengoen"/>
    <RelativeLayout
        android:id="@+id/relative9"
        android:layout_below="@id/im_shengfenzhengoen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传身份证国徽面"
            android:textSize="@dimen/dp_14"
            android:textColor="@color/v_bank"/>
    </RelativeLayout>
    <ImageView
        android:layout_below="@id/relative9"
        android:id="@+id/im_shengfenzhengtwo"
        android:layout_width="@dimen/dp_202"
        android:layout_height="@dimen/dp_128"
        android:layout_marginLeft="@dimen/dp_85"
        android:layout_marginRight="@dimen/dp_89"
        android:src="@mipmap/shengfenzhengtwo"/>
    <RelativeLayout
        android:id="@+id/relative10"
        android:layout_below="@id/im_shengfenzhengtwo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传手持身份证照片"
            android:textSize="@dimen/dp_14"
            android:textColor="@color/v_bank"/>
    </RelativeLayout>
    <ImageView
        android:id="@+id/im_addphotooen"
        android:layout_below="@id/relative10"
        android:layout_width="@dimen/dp_147"
        android:layout_height="@dimen/dp_116"
        android:src="@mipmap/addphotooen"
        android:layout_marginLeft="@dimen/dp_25"/>
    <ImageView
        android:id="@+id/im_addphototwo"
        android:layout_below="@id/relative10"
        android:layout_width="@dimen/dp_147"
        android:layout_height="@dimen/dp_116"
        android:src="@mipmap/addphotooen"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_25"/>

    <View
        android:id="@+id/view7"
        android:layout_below="@id/im_addphototwo"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_8"
        android:background="@color/bt_regist_back"
        android:layout_marginTop="@dimen/dp_20"></View>
    <RelativeLayout
        android:id="@+id/relative11"
        android:layout_below="@id/view7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_14"
            android:layout_marginTop="@dimen/dp_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传您的营业执照照片与扫描件"
            android:textSize="@dimen/dp_14"
            android:textColor="@color/v_bank"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/relative12"
        android:layout_below="@id/relative11"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_135">
        <ImageView
            android:id="@+id/im_yingyezao1"
            android:layout_width="@dimen/dp_100"
            android:layout_height="@dimen/dp_130"
            android:layout_marginLeft="@dimen/dp_20"
            android:src="@mipmap/addphototwo"/>
        <ImageView
            android:layout_toRightOf="@id/im_yingyezao1"
            android:id="@+id/im_yingyezao2"
            android:layout_width="@dimen/dp_100"
            android:layout_height="@dimen/dp_130"
            android:layout_marginLeft="@dimen/dp_20"
            android:src="@mipmap/addphototwo"/>
        <ImageView

            android:layout_toRightOf="@+id/im_yingyezao2"
            android:id="@+id/im_yingyezao3"
            android:layout_width="@dimen/dp_100"
            android:layout_height="@dimen/dp_130"
            android:layout_marginLeft="@dimen/dp_20"
            android:src="@mipmap/addphototwo"/>
    </RelativeLayout>
    <RelativeLayout
        android:background="@color/bt_regist_back"
        android:layout_below="@id/relative12"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_218">
        <CheckBox
            android:id="@+id/im_xuanzhe"
            android:layout_width="@dimen/dp_11"
            android:layout_height="@dimen/dp_11"
            android:layout_marginLeft="@dimen/dp_23"
            android:layout_marginTop="@dimen/dp_10"/>
        <TextView
            android:id="@+id/tv_xuanzhe"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_5"
            android:layout_toRightOf="@id/im_xuanzhe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/dp_10"
            android:text="已阅读并同意"/>
        <TextView
            android:id="@+id/tv_xieyi"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_toRightOf="@id/tv_xuanzhe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/dp_10"
            android:text="《商家入驻服务协议》"
            android:textColor="@color/xieyi"/>
        <Button
            android:id="@+id/btn_sc"
            android:background="@color/lg_back"
            android:layout_marginTop="@dimen/dp_111"
            android:layout_width="@dimen/dp_345"
            android:layout_height="@dimen/dp_40"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:text="立即上传"
            android:textColor="@color/white"/>
    </RelativeLayout>
    </RelativeLayout>

    </RelativeLayout>
    </ScrollView>
</LinearLayout>