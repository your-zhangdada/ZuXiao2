<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.mine.fragment.CompareFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="@dimen/dp_0"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/im_fr_sfz"
                android:layout_marginLeft="@dimen/dp_47"
                android:layout_marginTop="@dimen/dp_24"
                android:layout_width="@dimen/dp_39"
                android:layout_height="@dimen/dp_29"
                android:src="@mipmap/shengfenzhengoen"/>
            <TextView
                android:id="@+id/tv_fe_sfz"
                android:layout_width="@dimen/dp_79"
                android:layout_height="wrap_content"
                android:text="法人省份证"
                android:gravity="center"
                android:layout_marginTop="@dimen/dp_13"
                android:layout_marginLeft="@dimen/dp_28"
                android:textSize="@dimen/dp_15"
                />
            <TextView
                android:id="@+id/tv_fr_rz"
                android:layout_width="@dimen/dp_74"
                android:layout_height="@dimen/dp_27"
                android:gravity="center"
                android:text="@string/go_audit"
                android:background="@drawable/present_record_shape"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginLeft="@dimen/dp_30"
                android:textSize="@dimen/dp_12"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dp_0"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/im_yy"
                android:layout_marginLeft="@dimen/dp_47"
                android:layout_marginTop="@dimen/dp_24"
                android:layout_width="@dimen/dp_39"
                android:layout_height="@dimen/dp_29"
                android:src="@mipmap/shebaokaone"/>
            <TextView
                android:id="@+id/tv_yy"
                android:layout_width="@dimen/dp_79"
                android:layout_height="wrap_content"
                android:text="营业执照"
                android:gravity="center"
                android:layout_marginTop="@dimen/dp_13"
                android:layout_marginLeft="@dimen/dp_28"
                android:textSize="@dimen/dp_15"
                />
            <TextView
                android:id="@+id/tv_yy_rz"
                android:layout_width="@dimen/dp_74"
                android:layout_height="@dimen/dp_27"
                android:text="@string/go_audit"
                android:background="@drawable/present_record_shape"
                android:gravity="center"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginLeft="@dimen/dp_30"
                android:textSize="@dimen/dp_12"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/dp_0"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/im_qy"
                android:layout_marginLeft="@dimen/dp_47"
                android:layout_marginTop="@dimen/dp_24"
                android:layout_width="@dimen/dp_39"
                android:layout_height="@dimen/dp_29"
                android:src="@mipmap/gudingzctwo"/>
            <TextView
                android:id="@+id/tv_qy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="企业信息认证"
                android:layout_marginTop="@dimen/dp_13"
                android:layout_marginLeft="@dimen/dp_28"
                android:textSize="@dimen/dp_15"
                />
            <TextView
                android:id="@+id/tv_qy_rz"
                android:layout_width="@dimen/dp_74"
                android:layout_height="@dimen/dp_27"
                android:text="@string/go_audit"
                android:background="@drawable/present_record_shape"
                android:gravity="center"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginLeft="@dimen/dp_30"
                android:textSize="@dimen/dp_12"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>